{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@f2bc56ca6a7edb6af2bfeda6abe6bc727d1c6d89/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "serve_ondemand": "deno run -A serve.ts"
  }
}
